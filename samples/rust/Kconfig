menuconfig SAMPLES_RUST
	bool "Rust samples"
	depends on RUST
	help
	  You can build sample Rust kernel code here.

	  If unsure, say N.

if SAMPLES_RUST

config SAMPLE_RUST_HELLO
	tristate "Hello"
	help
	  This option builds the Rust Hello module.

	  If unsure, say N.

config SAMPLE_RUST_EBBCHAR
	tristate "Ebbchar"
	help
	  This option builds the Rust ebbchar module.

	  If unsure, say N.


endif # SAMPLES_RUST
